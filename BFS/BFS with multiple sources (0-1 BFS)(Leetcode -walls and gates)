#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;
#define endl "\n"
#define pb push_back
#define rep(i, n) for (int i = 0; i < (n); ++i)
#define all(v) (v).begin(), (v).end()
ll dx[4] = {-1, 0, 1, 0};
ll dy[4] = {0, 1, 0, -1};
const ll N = 1e3 + 12;
ll v[N][N];
ll n;
queue<pair<ll, ll>> p;
int32_t main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);

    cin >> n;

    for (ll i = 1; i <= n; i++)
    {
        for (ll j = 1; j <= n; j++)
        {
            cin >> v[i][j];
            if (v[i][j] == 0)
            {
                p.push({i, j});
            }
        }
    }
    ll d = 0;
    while (!p.empty())
    {
        d++;
        for (ll k = p.size(); k > 0; k--)
        {
            pair<ll, ll> c = p.front();
            ll a = c.first, b = c.second;
            p.pop();
            // cout<<a<<" "<<b<<endl;
            for (ll i = 0; i < 4; i++)
            {
                ll x = a + dx[i];
                ll y = b + dy[i];
                if (x >= 1 && x <= n && y >= 1 && y <= n && v[x][y] == 100)
                {
                    v[x][y] = d;
                    p.push({x, y});
                }
            }
        }
    }
    for (ll i = 1; i <= n; i++)
    {
        for (ll j = 1; j <= n; j++)
        {
            cout << v[i][j] << " ";
        }
        cout << endl;
    }
}
