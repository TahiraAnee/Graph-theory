#include<stdio.h>

const int mx=1e4;
struct bellman{
    int u;
    int v;
    int wt;
};
int main()
{
    
    int v,e;
    scanf("%d %d",&v,&e);
     struct bellman V[v+1];
    int j=0;
    for(int i=0;i<e;i++)
    {
        int a,b,c;
        scanf("%d %d %d",&a,&b,&c);
        V[i].u=a;
        V[i].v=b;
        V[i].wt=c;
    }
    int s;
    scanf("%d",&s);
    int ans[v+1];
    for(int i=0;i<v+1;i++)
    {
        ans[i]=mx;
    }
    ans[s]=0;
   // cout<<V.size()<<endl;
    for(int i=0;i<v-1;i++)
    {
        for(int i=0;i<v;i++)
        {
            int sn=V[i].u;
            int dn=V[i].v;
            int wtsd=V[i].wt;

            if(ans[sn]!=mx && ans[sn]+wtsd<ans[dn])
            {
                ans[dn]=ans[sn]+wtsd;
            }
        }
    }

       for(int i=0;i<v;i++)
        {
            int sn=V[i].u;
            int dn=V[i].v;
            int wtsd=V[i].wt;

            if(ans[sn]!=1e8 && ans[sn]+wtsd<ans[dn])
            {
               printf("The graph has a negative cycle\n");
               return 0;
            }
        }
    
    for(int i=0;i<v;i++)printf("%d ",ans[i]);
    printf("\n");



}
